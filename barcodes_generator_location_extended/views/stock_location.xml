<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_location_form_extended" model="ir.ui.view">
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="barcodes_generator_location.view_location_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//group[@name='barcodes_generator_location']"
                position="inside"
            >
                <field
                    name="barcode_rule_id"
                    domain="[('generate_model', '=', 'stock.location')]"
                    invisible="1"
                />
            </xpath>
        </field>
    </record>
</odoo>
