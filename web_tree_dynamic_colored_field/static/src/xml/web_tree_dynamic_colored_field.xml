<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-inherit="web.ListRenderer.RecordRow" t-inherit-mode="extension">
        <xpath
            expr="//tr[@class='o_data_row']/t[@t-foreach='getColumns(record)']/t[@t-if=&quot;column.type === 'field'&quot;]/td"
            position="attributes"
        >
            <attribute name="t-att-style">
                getCellStyleColored(column, record)
            </attribute>
        </xpath>
    </t>
</templates>
